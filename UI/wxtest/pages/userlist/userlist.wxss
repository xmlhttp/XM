.texttop{ height: 10vh;border-bottom: #ddd solid 1rpx; position: relative}
.headimg{ position: absolute; left: 0.5vh; top:0.5vh; height: 9vh; width: 9vh}
.nickname{ position: absolute; left: 10.5vh; top: 1.3vh; height:3.2vh; line-height: 3.2vh; font-size: 0.9rem; color: #666 }
.nickname text{ color: #69f}
.listdesc{position: absolute; left: 10.5vh; top:5.7vh;height:3.2vh; line-height: 3.2vh; font-size: 0.8rem; color: #777;}
.cnum,.cmoney,.ctime{ padding-left: 10rpx;padding-right: 10rpx; font-family: Verdana; color: #69f; font-size: 0.9rem}
.cmoney{ color: #f96}
.ctime{ color: #9f6}

.list-result{position: relative; height: 89.9vh;background: #ebebeb }
.list-result scroll-view{position:absolute;bottom:0;left:0;right:0;top:0} 


.result-item{display: flex; flex-direction: column;padding: 10rpx 0; overflow: hidden;} 
.iteminfo{border-top: 1rpx solid #dcdcdc; border-bottom: 1rpx solid #dcdcdc;background: #fff; padding:20rpx} 

.devdes{height: 50rpx;line-height: 50rpx; color: #777; font-size: 1rem}
.itemtime{ height: 40rpx;line-height: 40rpx; color: #999; font-size: 0.7rem; font-family: Verdana}
.middes{flex-direction: row;display: flex; padding-top: 20rpx; padding-bottom: 20rpx; border-bottom: #eee 1rpx dashed; margin-bottom:20rpx}
.midmoney{ flex: 1;border-right:#eee 1rpx dashed}
.midtitle{ color: #888; font-size: 0.8rem; text-align: center; letter-spacing: 1rpx}
.midnumber{ color: #777; font-size: 1.4rem; text-align: center; letter-spacing: 0;}
.midno{  font-size: 1.2rem;font-family: Verdana}
.miduint{ font-size: 1rem}
.itemno{ flex: 1; font-size: 0.9rem; color: #666}
.sumdes,.orderdes,.itemtit{line-height: 40rpx; color: #777; font-size: 0.8rem; padding-bottom:5px;word-break: break-all}
.ctit{ color: #999}
.ffont{ font-family: Verdana; font-size: 0.82rem}
.cstat0{color: #666}
.cstat1{color: #6c6}
.cstat2{color: #c66}
.loading{  
  padding: 10rpx;  
  text-align: center;  
  font-size: 0.9rem; color: #666
}  
.loading:before{  
  display: inline-block;  
  margin-right: 10rpx;  
  vertical-align: middle;  
  content: '';  
  width: 40rpx;  
  height: 40rpx;  
  background:url(data:image/png;base64,) no-repeat;  
  background-size: contain;  
  animation: rotate 1s linear infinite;  
}  
.loading.complete:before{  
  display: none;  
}  
